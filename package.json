{
  "name": "js-starter",
  "version": "0.0.1",
  "description": "A boilerplate for starting js code",
  "keywords": [
    "js-starter",
    "boilerplate"
  ],
  "private": false,
  "main": "./src/server.js",
  "types": "dist/index.d.ts",
  "directories": {
    "test": "__tests__"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnoreatterns": [
      "/node_modules/"
    ]
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "eslint \"src/**/*.js\"",
    "start": "parcel ./src/index.html",
    "dev": "nodemon ./src/server.js",
    "host": "git push heroku develop:master",
    "tails": "heroku logs --tail",
    "migrations": "node ./src/models/db/migrations/createTables.js createTables"
  },
  "author": {
    "name": "Build For SDG",
    "email": "buildforsdg@andela.com"
  },
  "homepage": "https://github.com/BuildForSDG/js-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/BuildForSDG/js-starter.git"
  },
  "bugs": {
    "url": "https://github.com/BuildForSDG/js-starter/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.21.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "jsonwebtoken": "^8.5.1",
    "mailgen": "^2.0.13",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "postgres-date": "^1.0.5",
    "request": "^2.88.2",
    "sequelize": "^5.21.9",
    "uuidv4": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.3",
    "babel-jest": "^25.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.4.0",
    "parcel-bundler": "^1.12.4",
    "supertest": "^4.0.2"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
